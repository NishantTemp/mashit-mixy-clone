{
  "version": 2,
  "builds": [
    {
      "src": "app/api/**/*.ts",
      "use": "@vercel/node@5.3.6"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build@2.7.16",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    { "src": "/api/songs\\?q=(?<query>[^&]*)", "dest": "/app/api/songs/route?q=$query" },
    { "src": "/api/songs/download/(?<id>[^/]+)\\?searchTerm=(?<term>[^&]*)", "dest": "/app/api/songs/download/$id?searchTerm=$term" },
    { "src": "/api/(.*)", "dest": "/app/api/$1" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}